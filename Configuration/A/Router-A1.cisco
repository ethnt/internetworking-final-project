version 12.4no service timestamps log datetime msecno service timestamps debug datetime msecno service password-encryption!hostname Router-A1!!!enable secret 5 $1$mERr$hx5rVt7rPNoS4wqbXKX7m0!!!ip dhcp pool LocationA network 192.168.101.0 255.255.255.0 default-router 192.168.101.1 dns-server 192.168.101.50!!!no ip cefno ipv6 cef!!!!!!!!!!no ip domain-lookup!!spanning-tree mode pvst!!!!!!interface FastEthernet0/0 no ip address duplex auto speed auto shutdown!interface FastEthernet0/1 ip address 192.168.101.1 255.255.255.0 duplex auto speed auto!interface Serial0/0/0 ip address 192.168.100.21 255.255.255.252 clock rate 2000000!interface Serial0/0/1 ip address 192.168.100.18 255.255.255.252!interface Serial0/2/0 ip address 192.168.100.49 255.255.255.252 clock rate 2000000!interface Serial0/2/1 ip address 192.168.100.45 255.255.255.252!interface Vlan1 no ip address shutdown!router eigrp 307 network 192.168.100.0 network 192.168.101.0 no auto-summary!ip classless!ip flow-export version 9!!!!!!!line con 0 password cisco logging synchronous login!line aux 0!line vty 0 4 password cisco logging synchronous login!!!end